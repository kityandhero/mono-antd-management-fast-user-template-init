{
  "cSpell.words": [
    "appstore",
    "buble",
    "conventionalcommits",
    "cssnano",
    "cyntler",
    "Displayer",
    "docdash",
    "favicons",
    "formily",
    "IIFE",
    "lintstagedrc",
    "mindmap",
    "msdoc",
    "Multibranch",
    "packagejson",
    "pannable",
    "pmmmwh",
    "posty",
    "postz",
    "postz:cz",
    "precommit",
    "prestart",
    "prez",
    "prez:cz",
    "reactflow",
    "svgr",
    "tanem",
    "tinymce",
    "toastify",
    "zoomable"
  ],
  "eslint.packageManager": "pnpm",
  "git.ignoreLimitWarning": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/dist": true,
    "**/coverage": true,
    "**/docs": true,
    "**/es": true,
    "**/pnpm-lock.yaml": true
  }
}
